<h1>University Registrar</h1>
<hr />
<h2>Courses:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var course in Model)
    {
      <li><a href="/courses/@course.GetId()">@course.GetName()</a></li>
    }
  </ul>
}
<h4><a href="/courses/new">Add a new course</a></h4>
<h4><a href="/">Return home</a></h4>
